<?xml version="1.0" encoding="UTF-8"?>
<plugin
  id="RocketMQConsumer"
  iconfile="logo.png"
  description="Apache RocketMQ Consumer"
  tooltip="Read messages throug a specific topic from a RocketMQ stream"
