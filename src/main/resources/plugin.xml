  <libraries>
    <library name="pentaho-rocketmq-consumer.jar"/>
    <library name="lib/commons-lang3-3.4.jar"/>
    <library name="lib/fastjson-1.2.51.jar"/>
    <library name="lib/netty-all-4.0.42.Final.jar"/>
    <library name="lib/netty-tcnative-boringssl-static-1.1.33.Fork26.jar"/>
    <library name="lib/rocketmq-client-4.4.0.jar"/>
    <library name="lib/rocketmq-common-4.4.0.jar"/>
    <library name="lib/rocketmq-logging-4.4.0.jar"/>
    <library name="lib/rocketmq-remoting-4.4.0.jar"/>
  </libraries>

  <localized_category>
    <category locale="en_US">message</category>
  </localized_category>
  <localized_description>
    <description locale="en_US">Apache RocketMQ Consumer</description>
  </localized_description>
  <localized_tooltip>
    <tooltip locale="en_US">Read messages throug a specific topic from a RocketMQ stream</tooltip>
